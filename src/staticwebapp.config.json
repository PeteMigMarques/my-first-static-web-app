{
  "routes": [
    {
      "route": "/",
      "allowedRoles": ["authenticated", "administrator"],
      "serve": "/noaccess.html",
      "statusCode": 403
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/login",
      "statusCode": 302
    },
    "403": {
      "rewrite": "/noaccess.html"
    }
  },
  "navigationFallback": {
    "rewrite": "/index.html"
  }
}
