{
  "routes": [
    {
      "route": "/secret.html",
      "statusCode": 404
    }
  ],
  "responseOverrides": {
    "400": {
      "rewrite": "/invalid-invitation-error.html"
    },
    "401": {
      "statusCode": 302,
      "redirect": "/login"
    },
    "403": {
      "rewrite": "/noaccess.html"
    },
    "404": {
      "redirect": "/custom-404.html"
    }
  }
}