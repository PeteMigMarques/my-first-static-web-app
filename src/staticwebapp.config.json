{
  "responseOverrides": {
    "400": {
      "rewrite": "/invalid-invitation-error.html"
    },
    "401": {
      "statusCode": 302,
      "redirect": "/login"
    },
    "403": {
      "rewrite": "/noaccess.html"
    },
    "404": {
      "rewrite": "/custom-404.html"
    }
  },
  "routes": [
    {
      "route": "/secret.html",      
      "rewrite": "/noaccess.html"
    }
  ],
   "networking": {
    "allowedIpRanges": [
      "94.140.14.14/32"
    ]
  }
}
