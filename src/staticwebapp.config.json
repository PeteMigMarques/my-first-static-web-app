{
   "clientPrincipal": {
    "identityProvider": "github",
    "userId": "test-user-id",
    "userDetails": "test@example.com",
    "roles": ["authenticated"]
  },
  "routes": [
    {
      "route": "/",
      "allowedRoles": ["administrator"],
      "statusCode": 403   
  ],
  "responseOverrides": {
    "403": {
      "rewrite": "/noaccess.html"
    }
  },
  "navigationFallback": {
    "rewrite": "/index.html"
  }
}
