{
  "routes": [
    {
      "route": "/secret.html",
      "statusCode": 401
    }
  ],
  "responseOverrides": {
    "400": {
      "rewrite": "/invalid-invitation-error.html"
    },
    "401": {
      "statusCode": 302,
      "redirect": "/login"
    },
    "403": {
      "rewrite": "/noaccess.html"
    },
    "404": {
      "rewrite": "/custom-404.html"
    }
  },
  "networking": {
    "allowedIpRanges": [
      "0.0.0.0/32"
    ]
  }
}