{
  "routes": [
    {
      "route": "/",
      "allowedRoles": ["administrator"],
      "statusCode": 403,
      "serve": "/noaccess.html"
    {
      "route": "/",
      "allowedRoles": ["authenticated"]
    }
  ],
  "responseOverrides": {
    "403": {
      "rewrite": "/noaccess.html"
    }
  },
  "navigationFallback": {
    "rewrite": "/index.html"
  }
}
